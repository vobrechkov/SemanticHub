{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KernelMemory": {
    "ContentStorageType": "AzureBlobs",
    "TextGeneratorType": "AzureOpenAI",
    "EmbeddingGeneratorType": "AzureOpenAI",
    "MemoryDbType": "Postgres",
    "DataIngestion": {
      "OrchestrationType": "Distributed",
      "MemoryDbTypes": [ "Postgres" ],
      "DistributedOrchestration": {
        "QueueType": "SimpleQueues"
      }
    },
    "Retrieval": {
      "MemoryDbType": "Postgres",
      "SearchClient": {
        "MinRelevance": 0.0,
        "MaxResults": 100,
        "DefaultResults": 10
      }
    },
    "Services": {
      "AzureBlobs": {
        "Auth": "AzureIdentity"
      },
      "AzureOpenAIText": {
        "Auth": "AzureIdentity",
        "Deployment": "gpt-4o-mini"
      },
      "AzureOpenAIEmbedding": {
        "Auth": "AzureIdentity",
        "Deployment": "text-embedding-3-small"
      },
      "AzureQueues": {
        "Auth": "AzureIdentity"
      },
      "Postgres": {
        "TablePrefix": "km_",
        "VectorSize": 1536
      }
    }
  }
}
